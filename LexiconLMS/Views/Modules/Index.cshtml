@model IEnumerable<LexiconLMS.Models.Module>

@{
    ViewBag.Title = "Index";
}

<h2>My Modules</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleStartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleEndDate)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.ModuleName, "Details", new { id = item.ModuleId })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleEndDate)
            </td>
        </tr>
    }

</table>
